\b 10. \uc0\u932 \u949 \u955 \u953 \u954 \u959 \u943  \u967 \u961 \u942 \u963 \u964 \u949 \u962  \u964 \u959 \u965  \u916 \u951 \u956 \u959 \u963 \u943 \u959 \u965 .
\b0  \uc0\u932 \u959  \u923 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u972  Apple \u954 \u945 \u953  \u951  \u963 \u967 \u949 \u964 \u953 \u954 \u942  \u964 \u949 \u954 \u956 \u951 \u961 \u943 \u969 \u963 \u951  \u945 \u960 \u959 \u964 \u949 \u955 \u959 \u973 \u957  \'ab\u917 \u956 \u960 \u959 \u961 \u953 \u954 \u940  \u949 \u943 \u948 \u951 \'bb, \u972 \u960 \u969 \u962  \u951  \u941 \u957 \u957 \u959 \u953 \u945  \u945 \u965 \u964 \u942  \u959 \u961 \u943 \u950 \u949 \u964 \u945 \u953  \u963 \u964 \u959  \u940 \u961 \u952 \u961 \u959  48, \u960 \u945 \u961 \u940 \u947 \u961 \u945 \u966 \u959 \u962  2.101 \u964 \u959 \u965  \u922 \u974 \u948 \u953 \u954 \u945  \u959 \u956 \u959 \u963 \u960 \u959 \u957 \u948 \u953 \u945 \u954 \u974 \u957  \u954 \u945 \u957 \u959 \u957 \u953 \u963 \u956 \u974 \u957  (C.F.R.), \u954 \u945 \u953  \u963 \u965 \u957 \u943 \u963 \u964 \u945 \u957 \u964 \u945 \u953  \u963 \u949  \'ab\u917 \u956 \u960 \u959 \u961 \u953 \u954 \u972  \u955 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u972  \u965 \u960 \u959 \u955 \u959 \u947 \u953 \u963 \u964 \u974 \u957 \'bb \u954 \u945 \u953  \'ab\u928 \u955 \u951 \u961 \u959 \u966 \u959 \u961 \u953 \u945 \u954 \u972  \u965 \u955 \u953 \u954 \u972  \u949 \u956 \u960 \u959 \u961 \u953 \u954 \u959 \u973  \u955 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u959 \u973  \u965 \u960 \u959 \u955 \u959 \u947 \u953 \u963 \u964 \u974 \u957 \'bb, \u972 \u960 \u969 \u962  \u959 \u953  \u941 \u957 \u957 \u959 \u953 \u949 \u962  \u945 \u965 \u964 \u941 \u962  \u959 \u961 \u943 \u950 \u959 \u957 \u964 \u945 \u953 , \u954 \u945 \u964 \u940  \u960 \u949 \u961 \u943 \u960 \u964 \u969 \u963 \u951 , \u963 \u964 \u959  \u940 \u961 \u952 \u961 \u959  48, \u960 \u945 \u961 \u940 \u947 \u961 \u945 \u966 \u959 \u962  12.212 \u942  \u963 \u964 \u959  \u940 \u961 \u952 \u961 \u959  48, \u960 \u945 \u961 \u940 \u947 \u961 \u945 \u966 \u959 \u962  227.7202 \u964 \u959 \u965  C.F.R.  \u931 \u973 \u956 \u966 \u969 \u957 \u945  \u956 \u949  \