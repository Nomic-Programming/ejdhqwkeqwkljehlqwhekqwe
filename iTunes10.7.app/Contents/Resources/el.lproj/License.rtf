\b0 \uc0\u919  \u949 \u966 \u945 \u961 \u956 \u959 \u947 \u942  \u945 \u965 \u964 \u942  \u960 \u949 \u961 \u953 \u941 \u967 \u949 \u953  \u955 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u972  \u964 \u951 \u962  Gracenote, Inc. 2000 Powell Street, Suite. 1380, Emeryville, California 94608 ("Gracenote"). \u932 \u959  \u955 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u972  \u964 \u951 \u962  Gracenote (\'ab\u923 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u972  Gracenote\'bb) \u960 \u945 \u961 \u941 \u967 \u949 \u953  \u963 \u964 \u951 \u957  \u960 \u945 \u961 \u959 \u973 \u963 \u945  \u949 \u966 \u945 \u961 \u956 \u959 \u947 \u942  \u964 \u951  \u948 \u965 \u957 \u945 \u964 \u972 \u964 \u951 \u964 \u945  \u951 \u955 \u949 \u954 \u964 \u961 \u959 \u957 \u953 \u954 \u942 \u962  \u964 \u945 \u965 \u964 \u959 \u960 \u959 \u943 \u951 \u963 \u951 \u962  \u948 \u943 \u963 \u954 \u969 \u957  \u954 \u945 \u953  \u945 \u961 \u967 \u949 \u943 \u969 \u957  \u956 \u959 \u965 \u963 \u953 \u954 \u942 \u962  \u954 \u945 \u953  \u964 \u951  \u955 \u942 \u968 \u951  \u956 \u959 \u965 \u963 \u953 \u954 \u974 \u957  \u960 \u955 \u951 \u961 \u959 \u966 \u959 \u961 \u953 \u974 \u957 , \u963 \u964 \u953 \u962  \u959 \u960 \u959 \u943 \u949 \u962  \u963 \u965 \u956 \u960 \u949 \u961 \u953 \u955 \u945 \u956 \u946 \u940 \u957 \u959 \u957 \u964 \u945 \u953  \u964 \u959  \u972 \u957 \u959 \u956 \u945 , \u959  \u954 \u945 \u955 \u955 \u953 \u964 \u941 \u967 \u957 \u951 \u962 , \u964 \u959  \u954 \u959 \u956 \u956 \u940 \u964 \u953  \u954 \u945 \u953  \u959  \u964 \u943 \u964 \u955 \u959 \u962  (\'ab\u916 \u949 \u948 \u959 \u956 \u941 \u957 \u945  Gracenote\'bb) \u945 \u960 \u972  \u963 \u965 \u957 \u948 \u949 \u948 \u949 \u956 \u941 \u957 \u959 \u965 \u962  \u948 \u953 \u945 \u954 \u959 \u956 \u953 \u963 \u964 \u941 \u962  (\'ab\u916 \u953 \u945 \u954 \u959 \u956 \u953 \u963 \u964 \u941 \u962  Gracenote\'bb), \u954 \u945 \u953  \u964 \u951  \u948 \u965 \u957 \u945 \u964 \u972 \u964 \u951 \u964 \u9