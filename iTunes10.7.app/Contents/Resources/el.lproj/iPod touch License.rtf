\b 12. \uc0\u917 \u966 \u945 \u961 \u956 \u959 \u963 \u964 \u941 \u959  \u948 \u943 \u954 \u945 \u953 \u959  \u954 \u945 \u953  \u945 \u954 \u965 \u961 \u972 \u964 \u951 \u964 \u945  \u949 \u960 \u953 \u956 \u941 \u961 \u959 \u965 \u962  \u972 \u961 \u969 \u957 . 
\b0 \uc0\u919  \u960 \u945 \u961 \u959 \u973 \u963 \u945  \u902 \u948 \u949 \u953 \u945  \u935 \u961 \u942 \u963 \u951 \u962  \u948 \u953 \u941 \u960 \u949 \u964 \u945 \u953  \u945 \u960 \u972  \u954 \u945 \u953  \u949 \u961 \u956 \u951 \u957 \u949 \u973 \u949 \u964 \u945 \u953  \u963 \u973 \u956 \u966 \u969 \u957 \u945  \u956 \u949  \u964 \u959  \u948 \u943 \u954 \u945 \u953 \u959  \u964 \u951 \u962  \u928 \u959 \u955 \u953 \u964 \u949 \u943 \u945 \u962  \u964 \u951 \u962  \u922 \u945 \u955 \u953 \u966 \u972 \u961 \u957 \u953 \u945 , \u949 \u958 \u945 \u953 \u961 \u974 \u957 \u964 \u945 \u962  \u964 \u951  \u963 \u973 \u947 \u954 \u961 \u959 \u965 \u963 \u951  \u964 \u969 \u957  \u945 \u961 \u967 \u974 \u957  \u957 \u972 \u956 \u959 \u965 . \u919  \u960 \u945 \u961 \u959 \u973 \u963 \u945  \u902 \u948 \u949 \u953 \u945  \u935 \u961 \u942 \u963 \u951 \u962  \u948 \u949 \u957  \u948 \u953 \u941 \u960 \u949 \u964 \u945 \u953  \u945 \u960 \u972  \u964 \u951 \u957  \u931 \u973 \u956 \u946 \u945 \u963 \u951  \u964 \u959 \u965  \u927 \u961 \u947 \u945 \u957 \u953 \u963 \u956 \u959 \u973  \u964 \u969 \u957  \u919 \u957 \u969 \u956 \u941 \u957 \u969 \u957  \u917 \u952 \u957 \u974 \u957  \u947 \u953 \u945  \u964 \u953 \u962  \u948 \u953 \u949 \u952 \u957 \u949 \u943 \u962  \u960 \u969 \u955 \u942 \u963 \u949 \u953 \u962  \u954 \u953 \u957 \u951 \u964 \u974 \u957  (\u931 \u973 \u956 \u946 \u945 \u963 \u951  \u964 \u951 \u962  \u914 \u953 \u941 \u957 \u957 \u951 \u962 ), \u951  \u949 \u966 \u945 \u961 \u956 \u959 \u947 \u942  \u964 \u951 \u962  \u959 \u960 \u959 \u943 \u945 \u962  \u945 \u960 \u959 \u954 \u955 \u949 \u943 \u949 \u964 \u945 \u953  \u961 \u951 \u964 \u974 \u962 . \u913 \u957  \u949 \u943 \u963 \u964 \u949  \u954 \u945 \u964 \u945 \u957 \u945 \u955 \u969 \u964 \u942 \u962  \u956 \u949  \u941 \u948 \u961 \u945  \u963 \u964 \u959  \u919 \u957 \u969 \u956 \u941 \u957 \u959  \u914 \u945 \u963 \u943 \u955 \u949 \u953 \u959 , \u951  \u960 \u945 \u961 \u959 \u973 \u963 \u945  \u902 \u948 \u949 \u953 \u945  \u948 \u953 \u941 \u960 \u949 \u964 \u945 \u953  \u945 \u960 \u972  \u964 \u959 \u965 \u962  \u957 \u972 \u956 \u959 \u965 \u962  \u964 \u951 \u962  \u948 \u953 \u954 \u945 \u953 \u959 \u948 \u959 \u963 \u943 \u945 \u962  \u964 \u951 \u962  \u967 \u974 \u961 \u945 \u962  \u954 \u945 \u964 \u959 \u953 \u954 \u943 \u945 \u962  \u963 \u945 \u962 . \u913 \u957  \u947 \u953 \u945  \u959 \u960 \u959 \u953 \u959 \u957 \u948 \u942 \u960 \u959 \u964 \u949  \u955 \u972 \u947 \u959  \u941 \u957 \u945  \u948 \u953 \u954 \u945 \u963 \u964 \u942 \u961 \u953 \u959  \u964 \u951 \u962  \u945 \u961 \u956 \u972 \u948 \u953 \u945 \u962  \u948 \u953 \u954 \u945 \u953 \u959 \u948 \u959 \u963 \u943 \u945 \u962  \u954 \u961 \u943 \u957 \u949 \u953  \u972 \u964 \u953  \u959 \u960 \u959 \u953 \u959 \u963 \u948 \u942 \u960 \u959 \u964 \u949  \u972 \u961 \u959 \u962  \u942  \u956 \u941 \u961 \u959 \u962  \u945 \u965 \u964 \u959 \u973  \u949 \u943 \u957 \u945 \u953  \u940 \u954 \u965 \u961 \u959 \u962 , \u951  \u965 \u960 \u972 \u955 \u959 \u953 \u960 \u951  \u902 \u948 \u949 \u953 \u945  \u935 \u961 \u942 \u963 \u951 \u962  \u960 \u945 \u961 \u945 \u956 \u941 \u957 \u949 \u953  \u963 \u949  \u960 \u955 \u942 \u961 \u951  \u953 \u963 \u967 \u973  \u954 \u945 \u953  \u949 \u958 \u945 \u954 \u959 \u955 \u959 \u965 \u952 \u949 \u943  \u957 \u945  \u960 \u945 \u961 \u940 \u947 \u949 \u953  \u964 \u945  \u945 \u960 \u959 \u964 \u949 \u955 \u941 \u963 \u956 \u945 \u964 \u940  \u964 \u951 \u962 .\
