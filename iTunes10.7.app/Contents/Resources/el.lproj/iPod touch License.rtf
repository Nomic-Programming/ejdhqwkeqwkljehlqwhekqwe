\b 6. \uc0\u923 \u973 \u963 \u951  \u964 \u951 \u962  \u963 \u973 \u956 \u946 \u945 \u963 \u951 \u962 . 
\b0 \uc0\u919  \u960 \u945 \u961 \u959 \u973 \u963 \u945  \u902 \u948 \u949 \u953 \u945  \u935 \u961 \u942 \u963 \u951 \u962  \u953 \u963 \u967 \u973 \u949 \u953  \u956 \u941 \u967 \u961 \u953  \u964 \u951  \u955 \u973 \u963 \u951  \u964 \u951 \u962 . \u932 \u945  \u948 \u953 \u954 \u945 \u953 \u974 \u956 \u945 \u964 \u940  \u963 \u945 \u962  \u948 \u965 \u957 \u940 \u956 \u949 \u953  \u964 \u951 \u962  \u960 \u945 \u961 \u959 \u973 \u963 \u945 \u962  \u902 \u948 \u949 \u953 \u945 \u962  \u935 \u961 \u942 \u963 \u951 \u962  \u945 \u943 \u961 \u959 \u957 \u964 \u945 \u953  \u945 \u965 \u964 \u972 \u956 \u945 \u964 \u945  \u942  \u949 \u953 \u948 \u940 \u955 \u955 \u969 \u962  \u960 \u945 \u973 \u959 \u965 \u957  \u957 \u945  \u953 \u963 \u967 \u973 \u959 \u965 \u957  \u967 \u969 \u961 \u943 \u962  \u960 \u961 \u959 \u949 \u953 \u948 \u959 \u960 \u959 \u943 \u951 \u963 \u951  \u945 \u960 \u972  \u964 \u951 \u957  Apple \u963 \u949  \u960 \u949 \u961 \u943 \u960 \u964 \u969 \u963 \u951  \u956 \u951  \u963 \u965 \u956 \u956 \u972 \u961 \u966 \u969 \u963 \u942 \u962  \u963 \u945 \u962  \u956 \u949  \u959 \u960 \u959 \u953 \u959 \u957 \u948 \u942 \u960 \u959 \u964 \u949  \u972 \u961 \u959  \u942  \u972 \u961 \u959 \u965 \u962  \u964 \u951 \u962  \u960 \u945 \u961 \u959 \u973 \u963 \u945 \u962  \u902 \u948 \u949 \u953 \u945 \u962  \u935 \u961 \u942 \u963 \u951 \u962 . \u924 \u949 \u964 \u940  \u964 \u951  \u955 \u973 \u963 \u951  \u964 \u951 \u962  \u940 \u948 \u949 \u953 \u945 \u962  \u967 \u961 \u942 \u963 \u951 \u962  \u959 \u966 \u949 \u943 \u955 \u949 \u964 \u949  \u957 \u945  \u948 \u953 \u945 \u954 \u972 \u968 \u949 \u964 \u949  \u954 \u940 \u952 \u949  \u967 \u961 \u942 \u963 \u951  \u964 \u959 \u965  \u923 \u959 \u947 \u953 \u963 \u956 \u953 \u954 \u959 \u973  iOS. \u927 \u953  \u949 \u957 \u972 \u964 \u951 \u964 \u949 \u962  4, 5, 6, 7, 8, 9, 12 \u954 \u945 \u953  13 \u964 \u951 \u962  \u960 \u945 \u961 \u959 \u973 \u963 \u945 \u962  \u902 \u948 \u949 \u953 \u945 \u962  \u935 \u961 \u942 \u963 \u951 \u962  \u949 \u958 \u945 \u954 \u959 \u955 \u959 \u965 \u952 \u959 \u973 \u957  \u957 \u945  \u953 \u963 \u967 \u973 \u959 \u965 \u957  \u963 \u949  \u960 \u949 \u961 \u943 \u960 \u964 \u969 \u963 \u951  \u959 \u960 \u959 \u953 \u959 \u965 \u948 \u942 \u960 \u959 \u964 \u949  \u949 \u957  \u955 \u972 \u947 \u969  \u964 \u949 \u961 \u956 \u945 \u964 \u953 \u963 \u956 \u959 \u973 .\
