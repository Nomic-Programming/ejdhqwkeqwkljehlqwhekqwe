<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Lista albumów</themeName>
	<themeExplanation>Drukuje album dla każdego utworu z zaznaczonej listy utworów lub biblioteki, sortowane według wykonawcy. Nie drukuje tytułów utworów.</themeExplanation>
	<themeKind>albumlist</themeKind>
	<themeInternalKind>playlist</themeInternalKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<albumlist>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<columnName>Liczba rzeczy</columnName>
			<field>trackNumber</field>
			<width>25</width>
		</column>
		<column>
			<columnName>Łączny czas</columnName>
			<field>totalTime</field>
			<width>50</width>
		</column>
	</albumlist>
</theme>
