<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Daty odtwarzania</themeName>
	<themeExplanation>Drukuje nazwę, długość, wykonawcę, album, liczbę odtworzeń oraz datę dla każdego utworu z zaznaczonej listy utworów lub biblioteki (w orientacji poziomej).</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>3</themeSortOrder>
	<themeMediaIndex>2</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>playDate</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>playCount</field>
			<width>50</width>
		</column>
	</playlist>
</theme>
