			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>playDate</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>playCount</field>
			<width>50</width>
		</column>
	</playlist>
</theme>
