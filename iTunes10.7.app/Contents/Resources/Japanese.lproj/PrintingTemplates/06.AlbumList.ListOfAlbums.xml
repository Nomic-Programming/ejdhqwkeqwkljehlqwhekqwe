<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>アルバムリスト</themeName>
	<themeExplanation>選択したプレイリストまたはライブラリにあるアルバムをアーティスト名順に並べ替えて印刷します。曲名は印刷されません。</themeExplanation>
	<themeKind>albumlist</themeKind>
	<themeInternalKind>playlist</themeInternalKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<albumlist>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<columnName>項目数</columnName>
			<field>trackNumber</field>
			<width>25</width>
		</column>
		<column>
			<columnName>合計時間</columnName>
			<field>totalTime</field>
			<width>50</width>
		</column>
	</albumlist>
</theme>
