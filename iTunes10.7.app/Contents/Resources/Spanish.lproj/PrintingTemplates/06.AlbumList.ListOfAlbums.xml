<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Lista de álbumes</themeName>
	<themeExplanation>Imprime los álbumes de la lista de reproducción o la biblioteca seleccionada, ordenados por artista. No incluye los títulos de las canciones.</themeExplanation>
	<themeKind>albumlist</themeKind>
	<themeInternalKind>playlist</themeInternalKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<albumlist>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<columnName>N.º de ítems</columnName>
			<field>trackNumber</field>
			<width>25</width>
		</column>
		<column>
			<columnName>Tiempo total</columnName>
			<field>totalTime</field>
			<width>50</width>
		</column>
	</albumlist>
</theme>
