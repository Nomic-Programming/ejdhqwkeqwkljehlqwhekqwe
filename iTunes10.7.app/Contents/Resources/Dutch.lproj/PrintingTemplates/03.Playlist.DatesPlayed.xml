<?xml version="1.0" encoding="utf-8"?>
<theme>
	<themeName>Afspeeldatums</themeName>
	<themeExplanation>Hiermee drukt u de titel, de duur, de artiest, het album, het aantal malen dat een nummer is afgespeeld en de laatste afspeeldatum af van de nummers uit de geselecteerde afspeellijst of bibliotheek. De afdrukrichting is liggend.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>3</themeSortOrder>
	<themeMediaIndex>2</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>playDate</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>playCount</field>
			<width>50</width>
		</column>
	</playlist>
</theme>