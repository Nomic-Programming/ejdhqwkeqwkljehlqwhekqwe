<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Morceaux</themeName>
	<themeExplanation>Cette option permet d’imprimer le nom, la durée, l’artiste et l’album des morceaux de la bibliothèque ou de la liste de lecture sélectionnée. Réglez l’orientation dans Format d’impression.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>1</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
	</playlist>
</theme>
