<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Classement de l’utilisateur</themeName>
	<themeExplanation>Cette option permet d’imprimer le nom, la durée, l’artiste, l’album et le classement des morceaux de la bibliothèque ou de la liste de lecture sélectionnée. L’impression est en orientation paysage.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>2</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>genre</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>userRating</field>
			<width>25</width>
		</column>
	</playlist>
</theme>
