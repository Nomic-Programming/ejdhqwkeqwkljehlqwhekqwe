<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Dates de lecture</themeName>
	<themeExplanation>Cette option permet d’imprimer le nom, la durée, l’artiste, l’album, le nombre de lectures et la date des morceaux de la liste de lecture ou de la bibliothèque sélectionnée. L’impression est en orientation paysage.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>3</themeSortOrder>
	<themeMediaIndex>2</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>playDate</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>playCount</field>
			<width>50</width>
		</column>
	</playlist>
</theme>
