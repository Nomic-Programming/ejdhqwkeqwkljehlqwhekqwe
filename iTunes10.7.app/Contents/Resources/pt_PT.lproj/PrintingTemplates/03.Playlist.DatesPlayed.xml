<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Datas de reprodução</themeName>
	<themeExplanation>Imprime nome, duração, intérprete, álbum, número e data de reprodução das músicas na lista ou biblioteca seleccionada. Imprime em orientação horizontal.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>3</themeSortOrder>
	<themeMediaIndex>2</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>playDate</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>playCount</field>
			<width>50</width>
		</column>
	</playlist>
</theme>
