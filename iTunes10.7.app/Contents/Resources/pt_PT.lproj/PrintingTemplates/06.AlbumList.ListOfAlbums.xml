<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Lista de álbuns</themeName>
	<themeExplanation>Imprime álbuns da lista ou biblioteca seleccionada, ordenados por intérprete. Não imprime os títulos das músicas. </themeExplanation>
	<themeKind>albumlist</themeKind>
	<themeInternalKind>playlist</themeInternalKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<albumlist>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<columnName>Elementos</columnName>
			<field>trackNumber</field>
			<width>25</width>
		</column>
		<column>
			<columnName>Duração</columnName>
			<field>totalTime</field>
			<width>50</width>
		</column>
	</albumlist>
</theme>
