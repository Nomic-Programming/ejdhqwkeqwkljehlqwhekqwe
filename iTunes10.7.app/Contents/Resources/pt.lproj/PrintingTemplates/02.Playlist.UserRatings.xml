<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Classificações do usuário</themeName>
	<themeExplanation>Imprime o nome, a duração, o artista, o álbum e a classificação das músicas de sua lista ou biblioteca selecionada. Imprime na orientação paisagem.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>2</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>genre</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>userRating</field>
			<width>25</width>
		</column>
	</playlist>
</theme>
