<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Lista de álbuns</themeName>
	<themeExplanation>Imprime os álbuns da sua lista ou biblioteca selecionada, ordenados por artista. Não imprime os nomes das músicas.</themeExplanation>
	<themeKind>albumlist</themeKind>
	<themeInternalKind>playlist</themeInternalKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<albumlist>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<columnName>Nº de Itens</columnName>
			<field>trackNumber</field>
			<width>25</width>
		</column>
		<column>
			<columnName>Duração Total</columnName>
			<field>totalTime</field>
			<width>50</width>
		</column>
	</albumlist>
</theme>
