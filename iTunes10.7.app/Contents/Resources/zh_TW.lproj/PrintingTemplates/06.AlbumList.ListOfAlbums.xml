<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>專輯列表</themeName>
	<themeExplanation>列印所選播放列表或資料庫中的專輯（依演出者排序）。不會列印歌曲名稱。</themeExplanation>
	<themeKind>albumlist</themeKind>
	<themeInternalKind>playlist</themeInternalKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<albumlist>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<columnName>項目數量</columnName>
			<field>trackNumber</field>
			<width>25</width>
		</column>
		<column>
			<columnName>總時間</columnName>
			<field>totalTime</field>
			<width>50</width>
		</column>
	</albumlist>
</theme>
