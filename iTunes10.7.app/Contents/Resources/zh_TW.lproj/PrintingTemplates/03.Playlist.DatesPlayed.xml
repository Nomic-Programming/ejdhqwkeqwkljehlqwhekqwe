<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>播放日期</themeName>
	<themeExplanation>列印所選播放列表或資料庫中歌曲的名稱、長度、演出者、專輯、播放次數和日期。以橫向列印。</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>3</themeSortOrder>
	<themeMediaIndex>2</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>playDate</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>playCount</field>
			<width>50</width>
		</column>
	</playlist>
</theme>
