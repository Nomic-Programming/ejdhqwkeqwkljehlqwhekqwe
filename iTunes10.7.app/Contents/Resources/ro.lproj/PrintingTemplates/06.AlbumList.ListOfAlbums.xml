<resizable />
<width>50</width>
</column>
<column>
<field>albumName</field>
<resizable />
<width>50</width>
</column>
<column>
<columnName># Items</columnName>
<field>trackNumber</field>
<width>25</width>
</column>
<column>
<columnName>Total Time</columnName>
<field>totalTime</field>
<width>50</width>
</column>
</albumlist>
</theme>
