<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>歌曲</themeName>
	<themeExplanation>打印您所选的播放列表或资料库中的歌曲的名称、长度、表演者和专辑。使用“页面设置”来控制打印方向。</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>1</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
	</playlist>
</theme>
