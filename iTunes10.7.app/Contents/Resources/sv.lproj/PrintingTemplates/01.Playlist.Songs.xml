<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Låtar</themeName>
	<themeExplanation>Skriver ut namn, tidslängd, artist och album för låtarna i markerad spellista eller bibliotek. Bestäm utskriftsriktning under Utskriftsformat.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>1</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
	</playlist>
</theme>
