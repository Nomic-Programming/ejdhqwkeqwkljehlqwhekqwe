<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Betyg</themeName>
	<themeExplanation>Skriver ut namn, tidslängd, artist, album och betyg för låtarna i markerad spellista eller bibliotek. Skrivs ut i liggande riktning.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>2</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>genre</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>userRating</field>
			<width>25</width>
		</column>
	</playlist>
</theme>
