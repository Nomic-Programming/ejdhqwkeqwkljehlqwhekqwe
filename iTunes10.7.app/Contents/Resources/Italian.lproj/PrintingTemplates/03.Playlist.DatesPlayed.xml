<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Date di riproduzione</themeName>
	<themeExplanation>Stampa il nome, la durata, l'artista, l'album, numero di riproduzioni e la data dei brani della playlist o libreria selezionata. Stampa in orientamento orizzontale.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>3</themeSortOrder>
	<themeMediaIndex>2</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>playDate</field>
			<resizable />
			<width>30</width>
		</column>
		<column>
			<field>playCount</field>
			<width>50</width>
		</column>
	</playlist>
</theme>
