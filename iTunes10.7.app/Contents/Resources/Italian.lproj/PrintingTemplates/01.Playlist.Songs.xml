<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Brani</themeName>
	<themeExplanation>Stampa il nome, la durata, l'artista e l'album dei brani della playlist o libreria selezionata. Utilizza Formato di stampa per controllare l'orientamento.</themeExplanation>
	<themeKind>playlist</themeKind>
	<themeSortOrder>1</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<playlist>
		<column>
			<field>trackName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>totalTime</field>
			<width>40</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
	</playlist>
</theme>
