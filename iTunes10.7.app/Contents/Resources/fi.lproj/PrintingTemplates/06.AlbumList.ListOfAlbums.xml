<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>Albumiluettelo</themeName>
	<themeExplanation>Tulostaa valitun soittolistan tai kirjaston albumit artistin mukaan järjestettynä. Ei tulosta kappaleiden nimiä.</themeExplanation>
	<themeKind>albumlist</themeKind>
	<themeInternalKind>playlist</themeInternalKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<albumlist>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<columnName>Kappalemäärä</columnName>
			<field>trackNumber</field>
			<width>25</width>
		</column>
		<column>
			<columnName>Kesto</columnName>
			<field>totalTime</field>
			<width>50</width>
		</column>
	</albumlist>
</theme>
