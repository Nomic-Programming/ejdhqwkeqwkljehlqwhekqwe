8.\ulnone  \ul \uc0\u3585 \u3634 \u3619 \u3592 \u3635 \u3585 \u3633 \u3604 \u3588 \u3623 \u3634 \u3617 \u3619 \u3633 \u3610 \u3612 \u3636 \u3604 \ulnone    \ul \uc0\u3648 \u3607 \u3656 \u3634 \u3607 \u3637 \u3656 \u3585 \u3598 \u3627 \u3617 \u3634 \u3618 \u3607 \u3637 \u3656 \u3648 \u3585 \u3637 \u3656 \u3618 \u3623 \u3586 \u3657 \u3629 \u3591 \u3617 \u3636 \u3652 \u3604 \u3657 \u3627 \u3657 \u3634 \u3617 \u3652 \u3623 \u3657  \u3652 \u3617 \u3656 \u3623 \u3656 \u3634 \u3585 \u3619 \u3603 \u3637 \u3651 \u3604 \u3654  Apple \u3652 \u3617 \u3656 \u3592 \u3635 \u3605 \u3657 \u3629 \u3591 \u3619 \u3633 \u3610 \u3612 \u3636 \u3604 \u3648 \u3614 \u3639 \u3656 \u3629 \u3585 \u3634 \u3619 \u3610 \u3634 \u3604 \u3648 \u3592 \u3655 \u3610 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3626 \u3656 \u3623 \u3609 \u3610 \u3640 \u3588 \u3588 \u3621 \u3627 \u3619 \u3639 \u3629 \u3588 \u3623 \u3634 \u3617 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3629 \u3633 \u3609 \u3648 \u3609 \u3639 \u3656 \u3629 \u3591 \u3617 \u3634 \u3592 \u3634 \u3585 \u3585 \u3634 \u3619 \u3612 \u3636 \u3604 \u3626 \u3633 \u3597 \u3597 \u3634  \u3588 \u3623 \u3634 \u3617 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3614 \u3636 \u3648 \u3624 \u3625  \u3588 \u3623 \u3634 \u3617 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3607 \u3634 \u3591 \u3629 \u3657 \u3629 \u3617  \u3627 \u3619 \u3639 \u3629 \u3588 \u3623 \u3634 \u3617 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3605 \u3656 \u3629 \u3648 \u3609 \u3639 \u3656 \u3629 \u3591   \u3595 \u3638 \u3656 \u3591 \u3619 \u3623 \u3617 \u3606 \u3638 \u3591 \u3649 \u3605 \u3656 \u3652 \u3617 \u3656 \u3592 \u3635 \u3585 \u3633 \u3604 \u3648 \u3593 \u3614 \u3634 \u3632 \u3588 \u3656 \u3634 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3648 \u3614 \u3639 \u3656 \u3629 \u3585 \u3634 \u3619 \u3626 \u3641 \u3597 \u3585 \u3635 \u3652 \u3619  \u3588 \u3623 \u3634 \u3617 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3627 \u3619 \u3639 \u3629 \u3585 \u3634 \u3619 \u3626 \u3641 \u3597 \u3627 \u3634 \u3618 \u3586 \u3629 \u3591 \u3586 \u3657 \u3629 \u3617 \u3641 \u3621 \u3627 \u3619 \u3639 \u3629 \u3626 \u3634 \u3619 \u3626 \u3609 \u3648 \u3607 \u3624  \u3585 \u3634 \u3619 \u3627 \u3618 \u3640 \u3604 \u3594 \u3632 \u3591 \u3633 \u3585 \u3586 \u3629 \u3591 \u3608 \u3640 \u3619 \u3585 \u3636 \u3592  \u3627 \u3619 \u3639 \u3629 \u3588 \u3623 \u3634 \u3617 \u3626 \u3641 \u3597 \u3627 \u3634 \u3618 \u3627 \u3619 \u3639 \u3629 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3607 \u3634 \u3591 \u3585 \u3634 \u3619 \u3588 \u3657 \u3634 \u3629 \u3639 \u3656 \u3609 \u3651 \u3604 \u3607 \u3637 \u3656 \u3648 \u3585 \u3636 \u3604 \u3592 \u3634 \u3585 \u3627 \u3619 \u3639 \u3629 \u3648 \u3585 \u3637 \u3656 \u3618 \u3623 \u3585 \u3633 \u3610 \u3585 \u3634 \u3619 \u3607 \u3637 \u3656 \u3607 \u3656 \u3634 \u3609 \u3651 \u3594 \u3657 \u3627 \u3619 \u3639 \u3629 \u3652 \u3617 \u3656 \u3626 \u3634 \u3617 \u3634 \u3619 \u3606 \u3651 \u3594 \u3657 \u3595 \u3629 \u3615 \u3649 \u3623 \u3619 \u3660 \u3586 \u3629 \u3591  Apple \u3627 \u3619 \u3639 \u3629 \u3610 \u3619 \u3636 \u3585 \u3634 \u3619 \u3627 \u3619 \u3639 \u3629 \u3595 \u3629 \u3615 \u3649 \u3623 \u3619 \u3660 \u3627 \u3619 \u3639 \u3629 \u3649 \u3629 \u3614 \u3614 \u3621 \u3636 \u3648 \u3588 \u3594 \u3633 \u3609 \u3651 \u3604 \u3654  \u3586 \u3629 \u3591 \u3610 \u3640 \u3588 \u3588 \u3621 \u3616 \u3634 \u3618 \u3609 \u3629 \u3585 \u3619 \u3656 \u3623 \u3617 \u3585 \u3633 \u3610 \u3595 \u3629 \u3615 \u3649 \u3623 \u3619 \u3660 \u3586 \u3629 \u3591  Apple \u3652 \u3617 \u3656 \u3623 \u3656 \u3634 \u3592 \u3632 \u3648 \u3585 \u3636 \u3604 \u3604 \u3657 \u3623 \u3618 \u3648 \u3627 \u3605 \u3640 \u3651 \u3604 \u3585 \u3655 \u3605 \u3634 \u3617  \u3650 \u3604 \u3618 \u3617 \u3636 \u3614 \u3633 \u3585 \u3605 \u3657 \u3629 \u3591 \u3588 \u3635 \u3609 \u3638 \u3591 \u3606 \u3638 \u3591 \u3607 \u3620 \u3625 \u3598 \u3637 \u3588 \u3623 \u3634 \u3617 \u3619 \u3633 \u3610 \u3612 \u3636 \u3604  (\u3651 \u3609 \u3617 \u3641 \u3621 \u3626 \u3633 \u3597 \u3597 \u3634  \u3617 \u3641 \u3621 \u3621 \u3632 \u3648 \u3617 \u3636 \u3604  \u3627 \u3619 \u3639 \u3629 \u3617 \u3641 \u3621 \u3629 \u3639 \u3656 \u3609 ) \u3649 \u3621 \u3632 \u3649 \u3617 \u3657  Apple \u3592 \u3632 \u3652 \u3604 \u3657 \u3619 \u3633 \u3610 \u3649 \u3592 \u3657 \u3591 \u3606 \u3638 \u3591 \u3588 \u3623 \u3634 \u3617 \u3648 \u3611 \u3655 \u3609 \u3652 \u3611 \u3652 \u3604 \u3657 \u3607 \u3637 \u3656 \u3592 \u3632 \u3648 \u3585 \u3636 \u3604 \u3588 \u3623 \u3634 \u3617 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3604 \u3633 \u3591 \u3585 \u3621 \u3656 \u3634 \u3623 \u3649 \u3621 \u3657 \u3623 \u3585 \u3655 \u3605 \u3634 \u3617  \u3610 \u3634 \u3591 \u3611 \u3619 \u3632 \u3648 \u3607 \u3624 \u3618 \u3633 \u3591 \u3652 \u3617 \u3656 \u3629 \u3609 \u3640 \u3597 \u3634 \u3605 \u3651 \u3627 \u3657 \u3592 \u3635 \u3585 \u3633 \u3604 \u3588 \u3623 \u3634 \u3617 \u3619 \u3633 \u3610 \u3612 \u3636 \u3604 \u3648 \u3614 \u3639 \u3656 \u3629 \u3585 \u3634 \u3619 \u3610 \u3634 \u3604 \u3648 \u3592 \u3655 \u3610 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3626 \u3656 \u3623 \u3609 \u3610 \u3640 \u3588 \u3588 \u3621  \u3588 \u3623 \u3634 \u3617 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3629 \u3633 \u3609 \u3648 \u3609 \u3639 \u3656 \u3629 \u3591 \u3617 \u3634 \u3592 \u3634 \u3585 \u3585 \u3634 \u3619 \u3612 \u3636 \u3604 \u3626 \u3633 \u3597 \u3597 \u3634 \u3627 \u3619 \u3639 \u3629 \u3588 \u3623 \u3634 \u3617 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3605 \u3656 \u3629 \u3648 \u3609 \u3639 \u3656 \u3629 \u3591  \u3604 \u3633 \u3591 \u3609 \u3633 \u3657 \u3609  \u3585 \u3634 \u3619 \u3592 \u3635 \u3585 \u3633 \u3604 \u3609 \u3637 \u3657 \u3629 \u3634 \u3592 \u3652 \u3617 \u3656 \u3609 \u3635 \u3617 \u3634 \u3651 \u3594 \u3657 \u3649 \u3585 \u3656 \u3607 \u3656 \u3634 \u3609 \ulnone  \uc0\u3652 \u3617 \u3656 \u3623 \u3656 \u3634 \u3585 \u3619 \u3603 \u3637 \u3651 \u3604 \u3654  \u3588 \u3623 \u3634 \u3617 \u3619 \u3633 \u3610 \u3612 \u3636 \u3604 \u3607 \u3633 \u3657 \u3591 \u3611 \u3623 \u3591 \u3586 \u3629 \u3591  Apple \u3605 \u3656 \u3629 \u3607 \u3656 \u3634 \u3609 \u3648 \u3614 \u3639 \u3656 \u3629 \u3588 \u3656 \u3634 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3607 \u3633 \u3657 \u3591 \u3611 \u3623 \u3591  (\u3609 \u3629 \u3585 \u3592 \u3634 \u3585 \u3607 \u3637 \u3656 \u3585 \u3598 \u3627 \u3617 \u3634 \u3618 \u3629 \u3634 \u3592 \u3585 \u3635 \u3627 \u3609 \u3604 \u3652 \u3623 \u3657 \u3651 \u3609 \u3585 \u3619 \u3603 \u3637 \u3648 \u3585 \u3637 \u3656 \u3618 \u3623 \u3585 \u3633 \u3610 \u3585 \u3634 \u3619 \u3610 \u3634 \u3604 \u3648 \u3592 \u3655 \u3610 \u3648 \u3626 \u3637 \u3618 \u3627 \u3634 \u3618 \u3626 \u3656 \u3623 \u3609 \u3610 \u3640 \u3588 \u3588 \u3621 ) \u3592 \u3632 \u3652 \u3617 \u3656 \u3648 \u3585 \u3636 \u3609 \u3585 \u3623 \u3656 \u3634 \u3627 \u3657 \u3634 \u3626 \u3636 \u3610  (50.00) \u3648 \u3627 \u3619 \u3637 \u3618 \u3597 \u3626 \u3627 \u3619 \u3633 \u3600  \u3586 \u3657 \u3629 \u3592 \u3635 \u3585 \u3633 \u3604 \u3586 \u3657 \u3634 \u3591 \u3605 \u3657 \u3609 \u3618 \u3656 \u3629 \u3617 \u3617 \u3637 \u3612 \u3621 \u3651 \u3594 \u3657 \u3610 \u3633 \u3591 \u3588 \u3633 \u3610 \u3649 \u3617 \u3657 \u3585 \u3634 \u3619 \u3649 \u3585 \u3657 \u3652 \u3586 \u3648 \u3618 \u3637 \u3618 \u3623 \u3618 \u3634 \u3607 \u3637 \u3656 \u3619 \u3632 \u3610 \u3640 \u3652 \u3623 \u3657 \u3586 \u3657 \u3634 \u3591 \u3605 \u3657 \u3609 \u3592 \u3632 \u3652 \u3617 \u3656 \u3648 \u3611 \u3655 \u3609 \u3652 \u3611 \u3605 \u3634 \u3617 \u3623 \u3633 \u3605 \u3606 \u3640 \u3611 \u3619 \u3632 \u3626 \u3591 \u3588 \u3660 \u3651 \u3609 \u3626 \u3634 \u3619 \u3632 \u3626 \u3635 \u3588 \u3633 \u3597 \
\
\ul 9.\ulnone  \ul \uc0\u3585 \u3634 \u3619 \u3588 \u3623 \u3610 \u3588 \u3640 \u3617 \u3585 \u3634 \u3619 \u3626 \u3656 \u3591 \u3629 \u3629 \u3585 \ulnone    \uc0\u3607 \u3656 \u3634 \u3609 \u3592 \u3632 \u3651 \u3594 \u3657 \u3627 \u3619 \u3639 \u3629 \u3626 \u3656 \u3591 \u3629 \u3629 \u3585 \u3627 \u3619 \u3639 \u3629 \u3626 \u3656 \u3591 \u3629 \u3629 \u3585 \u3652 \u3611 \u3629 \u3637 \u3585 \u3650 \u3604 \u3618 \u3611 \u3619 \u3632 \u3585 \u3634 \u3619 \u3629 \u3639 \u3656 \u3609 \u3595 \u3638 \u3656 \u3591 \u3595 \u3629 \u3615 \u3605 \u3660 \u3649 \u3623 \u3619 \u3660 \u3586 \u3629 \u3591  Apple \u3652 \u3617 \u3656 \u3652 \u3604 \u3657  \u3648 \u3623 \u3657 \u3609 \u3649 \u3605 \u3656 \u3585 \u3598 \u3627 \u3617 \u3634 \u3618 \u3626 \u3627 \u3619 \u3633 \u3600 \u3649 \u3621 \u3632 \u3585 \u3598 \u3627 \u3617 \u3634 \u3618 \u3611 \u3619 \u3632 \u3648 \u3607 \u3624 \u3607 \u3637 \u3656 \u3607 \u3656 \u3634 \u3609 \u3652 \u3604 \u3657 \u3595 \u3629 \u3615 \u3649 \u3623 \u3619 \u3660 \u3586 \u3629 \u3591  Apple \u3617 \u3634 \u3592 \u3632 \u3629 \u3609 \u3640 \u3597 \u3634 \u3605   \u3650 \u3604 \u3618 \u3648 \u3593 \u3614 \u3634 \u3632 \u3629 \u3618 \u3656 \u3634 \u3591 \u3618 \u3636 \u3656 \u3591  \u3595 \u3629 \u3615 \u3605 \u3660 \u3649 \u3623 \u3619 \u3660 \u3586 \u3629 \u3591  Apple \u3592 \u3632 \u3626 \u3656 \u3591 \u3629 \u3629 \u3585 \u3627 \u3619 \u3639 \u3629 \u3626 \u3656 \u3591 \u3629 \u3629 \u3585 \u3585 \u3621 \u3633 \u3610 \u3652 \u3611  (\u3585 ) \u3618 \u3633 \u3591 \u3611 \u3619 \u3632 \u3648 \u3607 \u3624 \u3651 \u3604 \u3654  \u3607 \u3637 \u3656 \u3626 \u3627 \u3619 \u3633 \u3600 \u3629 \u3648 \u3617 \u3619 \u3636 \u3585 \u3634 \u3626 \u3633 \u3656 \u3591 \u3627 \u3657 \u3634 \u3617 \u3626 \u3656 \u3591 \u3652 \u3611  \u3627 \u3619 \u3639 \u3629  (\u3586 ) \u3651 \u3627 \u3657 \u3610 \u3640 \u3588 \u3588 \u3621 \u3651 \u3604 \u3654  \u3607 \u3637 \u3656 \u3629 \u3618 \u3641 \u3656 \u3651 \u3609 \u3619 \u3634 \u3618 \u3594 \u3639 \u3656 \u3629 \u3612 \u3641 \u3657 \u3617 \u3637 \u3626 \u3633 \u3597 \u3594 \u3634 \u3605 \u3636 \u3595 \u3638 \u3656 \u3591 \u3619 \u3632 \u3610 \u3640 \u3652 \u3623 \u3657 \u3648 \u3611 \u3655 \u3609 \u3614 \u3636 \u3648 \u3624 \u3625 \u3586 \u3629 \u3591 \u3585 \u3619 \u3632 \u3607 \u3619 \u3623 \u3591 \u3585 \u3634 \u3619 \u3588 \u3621 \u3633 \u3591 \u3626 \u3627 \u3619 \u3633 \u3600 \u3627 \u3619 \u3639 \u3629 \u3619 \u3634 \u3618 \u3594 \u3639 \u3656 \u3629 \u3610 \u3640 \u3588 \u3588 \u3621 \u3627 \u3619 \u3639 \u3629 \u3629 \u3591 \u3588 \u3660 \u3585 \u3619 \u3607 \u3637 \u3656 \u3606 \u3641 \u3585 \u3611 \u3598 \u3636 \u3648 \u3626 \u3608 \u3650 \u3604 \u3618 \u3585 \u3619 \u3632 \u3607 \u3619 \u3623 \u3591 \u3614 \u3634 \u3603 \u3636 \u3594 \u3618 \u3660 \u3626 \u3627 \u3619 \u3633 \u3600 \u3652 \u3617 \u3656 \u3652 \u3604 \u3657  \u3648 \u3611 \u3655 \u3609 \u3605 \u3657 \u3609  \u3650 \u3604 \u3618 \u3585 \u3634 \u3619 \u3651 \u3594 \u3657 \u3595 \u3629 \u3615 \u3605 \u3660 \u3649 \u3623 \u3619 \u3660 \u3586 \u3629 \u3591  Apple  \u3607 \u3656 \u3634 \u3609 \u3619 \u3633 \u3610 \u3619 \u3629 \u3591 \u3649 \u3621 \u3632 \u3619 \u3633 \u3610 \u3611 \u3619 \u3632 \u3585 \u3633 \u3609 \u3623 \u3656 \u3634  \u3607 \u3656 \u3634 \u3609 \u3652 \u3617 \u3656 \u3611 \u3619 \u3632 \u3592 \u3635 \u3629 \u3618 \u3641 \u3656 \u3651 \u3609 \u3611 \u3619 \u3632 \u3648 \u3607 \u3624 \u3651 \u3604 \u3654  \u3604 \u3633 \u3591 \u3585 \u3621 \u3656 \u3634 \u3623  \u3627 \u3619 \u3639 \u3629 \u3617 \u3637 \u3594 \u3639 \u3656 \u3629 \u3611 \u3619 \u3634 \u3585 \u3599 \u3651 \u3609 \u3619 \u3634 \u3618 \u3594 \u3639 \u3656 \u3629 \u3651 \u3604 \u3654  \u3604 \u3633 \u3591 \u3585 \u3621 \u3656 \u3634 \u3623  \u3607 \u3656 \u3634 \u3609 \u3618 \u3633 \u3591 \u3605 \u3585 \u3621 \u3591 \u3604 \u3657 \u3623 \u3618 \u3623 \u3656 \u3634  \u3607 \u3656 \u3634 \u3609 \u3592 \u3632 \u3652 \u3617 \u3656 \u3651 \u3594 \u3657 \u3595 \u3629 \u3615 \u3605 \u3660 \u3649 \u3623 \u3619 \u3660 \u3586 \u3629 \u3591  Apple \u3648 \u3614 \u3639 \u3656 \u3629 \u3585 \u3634 \u3619 \u3651 \u3604 \u3654  \u3607 \u3637 \u3656 \u3585 \u3598 \u3627 \u3617 \u3634 \u3618 \u3626 \u3627 \u3619 \u3633 \u3600 \u3627 \u3657 \u3634 \u3617 \u3652 \u3623 \u3657   \u3595 \u3638 \u3656 \u3591 \u3619 \u3623 \u3617 \u3606 \u3638 \u3591 \u3650 \u3604 \u3618 \u3652 \u3617 \u3656 \u3592 \u3635 \u3585 \u3633 \u3604 \u3648 \u3593 \u3614 \u3634 \u3632 \u3585 \u3634 \u3619 \u3614 \u3633 \u3602 \u3609 \u3634  \u3629 \u3629 \u3585 \u3649 \u3610 \u3610  \u3607 \u3635  \u3627 \u3619 \u3639 \u3629 \u3612 \u3621 \u3636 \u3605 \u3586 \u3637 \u3611 \u3609 \u3634 \u3623 \u3640 \u3608 \u3627 \u3619 \u3639 \u3629  \u3629 \u3634 \u3623 \u3640 \u3608 \u3609 \u3636 \u3623 \u3648 \u3588 \u3621 \u3637 \u3618 \u3619 \u3660  \u3629 \u3634 \u3623 \u3640 \u3608 \u3648 \u3588 \u3617 \u3637  \u3627 \u3619 \u3639 \u3629 \u3629 \u3634 \u3623 \u3640 \u3608 \u3594 \u3637 \u3623 \u3616 \u3634 \u3614 \
