<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<themeName>รายการของอัลบั้ม</themeName>
	<themeExplanation>พิพม์อัลบั้มจากเพลย์ลีสต์หรือคลังที่เลือกของคุณที่จัดเรียงตามศิลปิน ไม่พิมพ์ชื่อเพลง</themeExplanation>
	<themeKind>albumlist</themeKind>
	<themeInternalKind>playlist</themeInternalKind>
	<themeSortOrder>2</themeSortOrder>
	<themeMediaIndex>1</themeMediaIndex>
	<albumlist>
		<column>
			<field>artistName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<field>albumName</field>
			<resizable />
			<width>50</width>
		</column>
		<column>
			<columnName># รายการ</columnName>
			<field>trackNumber</field>
			<width>25</width>
		</column>
		<column>
			<columnName>เวลาทั้งหมด</columnName>
			<field>totalTime</field>
			<width>50</width>
		</column>
	</albumlist>
</theme>
